<?xml version="1.0"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <requiredRuntime safemode="true" imageVersion="v4.0.30319" version="v4.0.30319"/>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
  <appSettings>
    <add key ="OperatorID" value ="admin"/> <!--当前操作员-->
    <add key="WorkStationID" value="DefaultStation"/>
    <add key ="ShowBalanceInterval" value="3"/> <!--远程读卡后到显示卡片余额的时间间隔-->
    <add key="RemoteReader" value="0"/> <!--远程读卡默认读头-->
    <!--工作站-->
    <add key="SystemType" value="0"/>
    <add key="ParkFeeLedCOMPort" value="0"/>
    <!--收费中心的LED串口号-->
    <add key="TicketReaderCOMPort" value="0"/>
    <!--收费中心的条码扫描串口号-->
    <add key="BillPrinterCOMPort" value="0"/>
    <!--小票打印机串口号-->
    <add key="Language" value=""/>
    <!--设置系统的语言 zh-CHS 表示中文简体  zh-CHT 表示中文繁体  en 表示英文-->
    <add key="AutoPrintSettleInfo" value="False"/>
    <!--设置是否在操作员结算时自动打印结算单-->
    <add key="EnableTTS" value="False"/>
    <!--是否启用电脑语音播报-->
    <add key="RememberLogID" value="True"/>
    <!--是否记住登录名-->
    <add key="NeedPasswordWhenExit" value="False"/>
    <!--是否退出需要登录密钥验证-->
    <add key="AuotAddToFirewallException" value="True"/>
    <!--是否自动添加系统防火墙例外-->
    <add key="CheckConnectionWithPing" value="True"/>
    <!--是否使用Ping网络来测试数据库连接-->
  </appSettings>
</configuration>
